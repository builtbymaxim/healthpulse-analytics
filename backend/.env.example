# HealthPulse Backend Environment Variables
# Copy this file to .env and fill in your values
# For Railway deployment: Add these as environment variables in Railway dashboard

# App Settings
DEBUG=false
API_VERSION=v1

# Supabase Configuration
# Get these from your Supabase project settings → API
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# JWT Secret (from Supabase project settings → API → JWT Settings)
JWT_SECRET=your-jwt-secret-here

# CORS Origins (use "*" to allow all origins including mobile apps)
CORS_ORIGINS_STR=*
